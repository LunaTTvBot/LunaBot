<Application x:Class="iBot_GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             Exit="App_OnExit"
             StartupUri="Forms/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Template/Themes/Color/Red.xaml" />
                <ResourceDictionary Source="Template/Themes/Color/Green.xaml" />
                <ResourceDictionary Source="Template/Themes/Light.xaml" />
                <ResourceDictionary Source="Template/Controls/Buttons/HeaderActionButton.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
