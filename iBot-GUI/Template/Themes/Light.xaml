<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="#FFF2F2F2"/>
    <Color x:Key="MainBackgroundColor" A="255" B="242" G="242" R="242"/>
    
    <SolidColorBrush x:Key="MainBackgroundBrushSecondary" Color="#FFCCCCCC"/>
    <Color x:Key="MainBackgroundColorSecondary" A="255" B="204" G="204" R="204"/>

    <SolidColorBrush x:Key="BorderBrush" Color="#FF555555"/>
    <Color x:Key="BorderColor" A="255" B="85" G="85" R="85"/>
    
    <SolidColorBrush x:Key="BorderBrushSecondary" Color="#FFCCCCCC"/>

    <SolidColorBrush x:Key="TextBrushPrimary" Color="#FF222222"/>
    <Color x:Key="TextColorPrimary" A="255" B="34" G="34" R="34"/>
    
    <SolidColorBrush x:Key="TextBrushSecondary" Color="#FF555555"/>

    <SolidColorBrush x:Key="TextBrushStatus" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="VectorBrush" Color="#222222"/>
    <Color x:Key="VectorColor" A="255" B="34" G="34" R="34"/>
    
    <SolidColorBrush x:Key="VectorActionPressedBrush" Color="#FFFFFFFF"/>
    
    <SolidColorBrush x:Key="WindowActionHoverBrush" Color="#FFCCCCCC"/>
</ResourceDictionary>