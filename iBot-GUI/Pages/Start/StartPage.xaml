<UserControl x:Class="iBot_GUI.Pages.Start.StartPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:iBot_GUI.Pages.Start"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="Root">
        <TextBox x:Name="InfoBox" FontFamily="Consolas" BorderThickness="0" HorizontalAlignment="Stretch"
                 TextWrapping="Wrap" Focusable="False" Height="auto" VerticalAlignment="Stretch" IsReadOnly="True"
                 AcceptsReturn="True" VerticalScrollBarVisibility="Auto" Foreground="{DynamicResource PrimaryHueLightBrush}" />
    </Grid>
</UserControl>